<div class="container-fluid">
  <% @posts.each do |post| %>
    <div class="row">
        <div class="jumbotron">
          <div class="page-header">
            <h1><%= post.title %> <small>by: <%= post.user.email %></small></h1>
            <p>submmited picture:</p>
            <%= link_to image_tag(post.image.url(:medium), class: 'img-circle'),
            post.image.url, target: '_blank' %>
          </div>
        </div>
    </div>

  <% end %>
</div>
